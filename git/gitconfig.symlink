[user]
	name = Joel Gillman
	email = joeltgillman@gmail.com

[alias]
	st = status
	co = checkout
	sp = stash pop
	di = diff
	dc = diff --cached
	back = reset --soft HEAD^
	root = !pwd

[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = true
	pager = true

[core]
	excludesfile = ~/.gitignore
	editor = vim

[github]
	user = jgillman

[push]
	default = current
[grep]
	extendRegexp = true
	lineNumber = true
[heroku]
	account = personal
