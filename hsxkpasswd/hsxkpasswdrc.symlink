{
  "custom_presets": {
    "DEFAULT": {
      "description" : "An iPhone friendly preset to override the standard default",
      "config": {
        "num_words": 3,
        "word_length_min": 4,
        "word_length_max": 8,
        "case_transform": "RANDOM",
        "separator_character": "RANDOM",
        "separator_alphabet": [
          "-", "/", ":", ";", "(", ")", "$", "&", "@", "\"", ".", ",", "?", "!", "'"
        ],
        "padding_digits_before": 2,
        "padding_digits_after": 2,
        "padding_type": "FIXED",
        "padding_character": "RANDOM",
        "symbol_alphabet": [
         "-", "/", ":", ";", "(", ")", "$", "&", "@", "\"", ".", ",", "?", "!", "'"
        ],
        "padding_characters_before": 2,
        "padding_characters_after": 2
      }
    }
  },
  "default_entropy_warnings": "ALL"
}

